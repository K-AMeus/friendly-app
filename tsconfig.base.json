{
  "compilerOptions": {
    "target": "ES2022",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules"]
}
